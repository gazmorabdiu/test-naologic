// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("db.url")
}

model User{
  id                     String        @id @default(auto()) @map("_id") @db.ObjectId
  roleName String
  firstName String
  lastName String
  bornAt DateTime
  gender String
  username String?
  password String?
  refreshToken String?
  

}
